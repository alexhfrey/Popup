
		
		<div class="container" id="main-content">
			<div class="row">
				<div class="span12">
				<div class="categorybox">
				<div id="headline">
					<h2><%= @event.title %> </h2>
					</div>
					
<div class="span8 columns">
<div id="myCarousel" class="carousel slide">

<!-- Carousel items -->
<div class="carousel-inner">

<div class="active item">
<%= image_tag @event.picture1_url, :class=> "pull-left" %>
</div>

<div class="item">
<%= image_tag @event.picture2_url, :class=> "pull-left" %>
</div>

</div>
<!-- Carousel nav -->
<a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
<a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
</div>
</div>

					<h3>About</h3>
					<p> <%= @event.description %> 
					</p>			
					<h3>When</h3>
					<p><%= @event.start %> to <%= @event.end %>
					</p>
					<h3>Open</h3>
					<p><%= @event.open %>
					</p>
					<h3>Organized By</h3>
					<p><%= @event.brand_name %> 
					</p>
					<button type="submit" class="btn btn-danger btn-large">RSVP for Event</button>
				</div>
				</div>
			</div>
		</div>
		
		
