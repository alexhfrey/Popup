
			
		
		<div class="container" id="main-content">
			<div class="row">
				<div class="span12" id="headline">
				<h2>Discover Amazing Pop up Shops in <%= @user.city %>.</h2>
					<p>
					Hey There, we're glad you're here. Love Popup is excited to introduce you to the hottest pop up shops in your city. We're just getting started, so if there are any bugs or you have suggestions, <a href="contact">send us a note</a>. Gracias!
					</p>
					</div>

					<ul class="nav nav-pills">
			<li><a href="/events/?cat=fashion"><h3>FASHION   <%= image_tag "fashionicon.png" %></h3></a></li>
			<li><a href="/events/?cat=food"><h3>FOOD         <%= image_tag "foodicon.png" %></h3></a></li>
			<li><a href="/events/?cat=art"><h3>ART           <%= image_tag "articon.png" %></h3></a></li>
			</ul>

					<div class="eventlist">
					<ul>
					<% @events .each do |e| %>
					
					<li><a href="/events/<%= e.id %>">
					<%= image_tag(e.picture1_url) %>
					<div class="eventtitlebox"></div>
					<span class="eventlisttitle"><%= e.title %></span>
					</a>
					</li>
					
					<% end %>
				</ul>
					
					</div>
					
				</div>
			</div>
		</div>
	